{
	"name": "ibs3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:backend": "nodemon --watch backend -e ts --exec \"ts-node-esm ./backend/index.ts\"",
		"dev:frontend": "vite dev --host 0.0.0.0",
		"dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.15.2",
		"@types/minio": "^7.0.17",
		"@types/node-cron": "^3.0.7",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"classnames": "^2.3.2",
		"concurrently": "^7.6.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.6.3",
		"flowbite-svelte": "^0.29.12",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.10.1",
		"sass": "^1.58.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-hamburgers": "^4.0.1",
		"svelte-heros-v2": "^0.3.11",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^0.2.0",
		"@prisma/client": "^4.9.0",
		"@types/express": "^4.17.17",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"next-auth": "^4.20.1",
		"node-cache": "^5.1.2",
		"node-cron": "^3.0.2",
		"nodemon": "^2.0.21",
		"svelte-modals": "^1.2.1",
		"ts-node": "^10.9.1",
		"zod": "^3.20.6"
	}
}
