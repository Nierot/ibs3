{
	"name": "ibs3",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev:backend": "nodemon --watch backend -e ts --exec \"ts-node-esm ./backend/index.ts\"",
		"dev:frontend": "vite dev --host 0.0.0.0",
		"dev:email-templates": "npx sass --watch ./emails/scss/style-test.sass ./build/css/style-test.css",
		"dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"log": "git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit",
		"mailhog": "docker run --rm -it -p 1025:1025 -p 8025:8025 --name mailhog mailhog/mailhog"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.47",
		"@popperjs/core": "^2.11.6",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-node": "^1.2.3",
		"@sveltejs/kit": "^1.15.2",
		"@types/express": "^4.17.17",
		"@types/markdown-it": "^12.2.3",
		"@types/markdown-it-emoji": "^2.0.2",
		"@types/minio": "^7.0.17",
		"@types/node-cron": "^3.0.7",
		"@types/sanitize-html": "^2.9.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"autoprefixer": "^10.4.13",
		"classnames": "^2.3.2",
		"concurrently": "^7.6.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"flowbite": "^1.6.3",
		"flowbite-svelte": "^0.29.12",
		"nodemon": "^3.0.1",
		"postcss": "^8.4.21",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"prisma": "^4.10.1",
		"sass": "^1.58.0",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"svelte-hamburgers": "^4.0.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"ts-node": "^10.9.1",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"unplugin-icons": "^0.16.1",
		"vite": "^4.0.5"
	},
	"type": "module",
	"dependencies": {
		"@auth/sveltekit": "^0.2.0",
		"@prisma/client": "^4.9.0",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"markdown-it": "^13.0.1",
		"markdown-it-emoji": "^2.0.2",
		"markdown-it-ins": "^3.0.1",
		"markdown-it-kbd": "^2.2.2",
		"markdown-it-smartarrows": "^1.0.1",
		"markdown-it-sub": "^1.0.0",
		"markdown-it-sup": "^1.0.0",
		"next-auth": "^4.20.1",
		"node-cache": "^5.1.2",
		"sanitize-html": "^2.11.0",
		"svelte-modals": "^1.2.1",
		"zod": "^3.20.6"
	}
}